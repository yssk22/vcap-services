service_name: mongodb
backup_base_dir: <%= BACKUP_DIR %>
local_db: <%= DB_URL %>
base_dir: /tmp/mongo/instances
mongodump_path:
  "1.8": <%= BINARY_DIR %>/mongodump
  "2.0": <%= BINARY_DIR %>/mongodump
  "2.2": <%= BINARY_DIR %>/mongodump
supported_versions: ["1.8", "2.0", "2.2"]
default_version: "2.2"
service_log_dir: <%= MONGOD_LOG %>
image_dir: /tmp/mongo/images
timeout: 10
logging:
  level: debug
